  "main": "peter_fonseca.jsx",
  "scripts": {
    "check-types": "tsc --skipLibCheck",
    "build": "npm run clean-dist && webpack-cli --mode production --config=config/webpack/prod.js",
    "build-with-stats": "npm run clean-dist && webpack-cli --mode production --config=config/webpack/prod.js --profile --json > dist/raw_stats.json",
    "clean-dist": "rm -rfd dist/*",
    "start-dev": "webpack serve --config=config/webpack/dev.js",
    "start-prod": "node --optimize_for_size --max_old_space_size=2560 express.js",
    "test": "jest --config=jest.config.js",
    "test-ci": "npm run check-types && npm run test"
